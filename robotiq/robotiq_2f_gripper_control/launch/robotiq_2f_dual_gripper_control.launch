<?xml version="1.0"?>
<launch>
    <arg name="deviceL" default="/dev/ttyUSB1"/>
    <arg name="deviceR" default="/dev/ttyUSB0"/>
    <node pkg="robotiq_2f_gripper_control" 
          name="L_Robotiq2FGripperRtuNode" 
          type="L_Robotiq2FGripperRtuNode.py" 
          output="screen"
          respawn="false"
          required="false"
          args="$(arg deviceL)">
    </node>

    <node pkg="robotiq_2f_gripper_control" 
          name="R_Robotiq2FGripperRtuNode" 
          type="R_Robotiq2FGripperRtuNode.py" 
          output="screen"
          respawn="false"
          required="false"
          args="$(arg deviceR)">
    </node>

</launch>